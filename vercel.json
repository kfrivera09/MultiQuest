{
  "version": 2,
  "builds": [
    {
      "src": "src/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api.js"
    },
    {
      "src": "/",
      "dest": "/html/home.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
